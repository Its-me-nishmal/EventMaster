  <script src="  https://printjs-4de6.kxcdn.com/print.min.js"></script>

</head>
{{!-- 3.5.1 --}}

<body>
  {{#if adminHeader}}
  {{>admin-header}}
  {{else}}{{#if groupHeader}}
  {{>group-header}}
  {{else}}
  {{>user-header}}
  {{/if}}{{/if}}
  {{#if eventHeader}}
  {{>event-header}}
  {{/if}}
  {{{body}}}
  {{!-- {{>admin-footer}} --}}
  {{#if footer}}
  {{>admin-footer}}
  {{/if}}

  {{#if Success}}
  <div style=" background-color: rgb(138, 255, 177); border-color:rgb(0, 165, 55); padding-right: 30px;" class="groupAlertBorder">
    <div class="groupAlert-box-close-button">
      <i class="bi bi-x-lg" onclick="closeAlet()"></i>
    </div>

    <div class="groupAletCountent" style="color: rgb(56, 95, 69); margin-top: 10px;">
      <h5>{{Success}}</h5>
    </div>
    <audio autoplay hidden src="/audio/alert/error alert.mp3"></audio>
  </div>
  {{/if}}
  {{#if Error}}
  <div id="close" class="Alert-box-border" style="padding-right: 30px;">
    <div>
      <div class="Alert-box-close-button">
        <i class="bi bi-x-lg" onclick="closeAlet()"></i>
      </div>
     
      <div class="alert-box-content" style="margin-top: 10px;">
        <h5 id="loginErr">{{Error}}</h5>
      </div>

      <audio id="alertAudio" autoplay hidden src="/audio/alert/error alert.mp3"></audio>
    </div>
  </div>
  {{/if}}
  <script>

    let Android = "Android"
    Android = new RegExp('(\\w*' + Android + '\\w*)', 'gi');
    let ios = "iphone"
    ios = new RegExp('(\\w*' + ios + '\\w*)', 'gi');
    var pho = document.getElementById('hi')
    var AndroidMobile = platform.description.match(Android);
    var iphone = platform.description.match(ios);
    if (AndroidMobile) {
      document.getElementById('mobile').href = "/stylesheets/group/media-mobile.css";

    } else if (iphone) {
      document.getElementById('mobile').href = "/stylesheets/group/media-mobile.css";

    } else {


    }

    function closeAlet() {

      var alert_box = document.getElementById('close')
      alert_box.style.display = "none"

    }

  </script>
</body>

</html>