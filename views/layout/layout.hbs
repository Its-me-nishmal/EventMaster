<body>
  {{#if adminHeader}}
  {{>admin-header}}
  {{else}}{{#if groupHeader}}
  {{>group-header}}
  {{else}}
  {{>user-header}}
  {{/if}}{{/if}}
  {{#if festHeader}}
  {{>fest-header}}
  {{/if}}
  {{{body}}}
  {{!-- {{>admin-footer}} --}}
  {{#if footer}}
  {{>admin-footer}}
  {{/if}}

</body>

</html>