<section>
    <div class="group-container">
        <div class="allGroupStudents-mainDiv">
            <div class="GroupAllCategory-header">
                <h4 class="adminHome-sectin-one-SubHeading">{{SessionName}} / {{Category}}</h4>
                <hr class="GroupallCategoryheader-hr">
            </div>
        </div>
        {{#if EventAlreadyUsedError}}
        <div class="groupAlertBorder">
            <div class="groupAlert-box-close-button">
                <i class="bi bi-x-lg" onclick="closeAlet()"></i>
            </div>
            <div class="groupAlertHead">
                <i class="bi bi-exclamation-circle"></i>
                <h4>Create failed !</h4>
            </div>
            <div class="groupAletCountent">
                <h5>{{EventAlreadyUsedError}}</h5>
            </div>
            <audio autoplay hidden src="/audio/alert/error alert.mp3"></audio>
        </div>
        {{/if}}
        {{#if StudentStageCountOver}}
        <div class="groupAlertBorder">
            <div class="groupAlert-box-close-button">
                <i class="bi bi-x-lg" onclick="closeAlet()"></i>
            </div>
            <div class="groupAlertHead">
                <i class="bi bi-exclamation-circle"></i>
                <h4>Create failed !</h4>
            </div>
            <div class="groupAletCountent">
                <h5>{{StudentStageCountOver}}</h5>
            </div>
            <audio autoplay hidden src="/audio/alert/error alert.mp3"></audio>
        </div>
        {{/if}}
        {{#if EventLimitError}}
        <div class="groupAlertBorder">
            <div class="groupAlert-box-close-button">
                <i class="bi bi-x-lg" onclick="closeAlet()"></i>
            </div>
            <div class="groupAlertHead">
                <i class="bi bi-exclamation-circle"></i>
                <h4>Create failed !</h4>
            </div>
            <div class="groupAletCountent">
                <h5>{{EventLimitError}}</h5>
            </div>
            <audio autoplay hidden src="/audio/alert/error alert.mp3"></audio>
        </div>
        {{/if}}
        {{#if ChestNOError}}
        <div class="groupAlertBorder">
            <div class="groupAlert-box-close-button">
                <i class="bi bi-x-lg" onclick="closeAlet()"></i>
            </div>
            <div class="groupAlertHead">
                <i class="bi bi-exclamation-circle"></i>
                <h4>Create failed !</h4>
            </div>
            <div class="groupAletCountent">
                <h5>{{ChestNOError}}</h5>
            </div>
            <audio autoplay hidden src="/audio/alert/error alert.mp3"></audio>
        </div>
        {{/if}}
        {{#if TotalCountError}}
        <div class="groupAlertBorder">
            <div class="groupAlert-box-close-button">
                <i class="bi bi-x-lg" onclick="closeAlet()"></i>
            </div>
            <div class="groupAlertHead">
                <i class="bi bi-exclamation-circle"></i>
                <h4>Create failed !</h4>
            </div>
            <div class="groupAletCountent">
                <h5>{{TotalCountError}}</h5>
            </div>
            <audio autoplay hidden src="/audio/alert/error alert.mp3"></audio>
        </div>
        {{/if}}
        {{#if Success}}
        <div style=" background-color: rgb(138, 255, 177); border-color:rgb(0, 165, 55)" class="groupAlertBorder">
            <div class="groupAlert-box-close-button">
                <i class="bi bi-x-lg" onclick="closeAlet()"></i>
            </div>
            <div class="groupAlertHead" style=" color: rgb(0, 126, 42);">
                <i class="bi bi-exclamation-circle"></i>
                <h4>Create Success !</h4>
            </div>
            <div class="groupAletCountent" style="color: rgb(56, 95, 69);">
                <h5>{{Success}}</h5>
            </div>
            <audio autoplay hidden src="/audio/alert/error alert.mp3"></audio>
        </div>
        {{/if}}
        <div class="GroupAllStudents-content">

            {{#if EventActiveUndifined}}
            <div class="GroupStudentsZero">
                <div class="pointTabel-div">
                    <div class="pointTable-IconDiv">
                        <i class="bi bi-hourglass-top"></i>
                    </div>
                    <p>Pleace Wait... <br> The time to create events has not begun</p>
                </div>
            </div>

            {{else}}
            <div class="GroupEvents-All-content " style="box-sizing: border-box;">
                {{#if AllEvents.[0]}}
                {{#each AllEvents}}
                <a id="mainDiv"
                    href="/group/events/{{../SessionName}}/{{../Category}}/{{this.EventId}}-{{this.EventName}}">
                    <div style="background-color: rgb(182, 224, 248);" class="allGroupCatetory-items">
                        <h4>{{this.EventName}} <span class="EventIdSpan">({{this.EventId}})</span> - ({{this.PointCategoryName}}) </h4>
                        <p>{{this.LimitStatus}}/{{this.EventLimit}}</p>
                    </div>
                </a>
                {{/each}}

                {{else}}
                <div class="GroupStudentsZero">
                    <div class="pointTabel-div">
                        <div class="pointTable-IconDiv">
                            <i class="bi bi-emoji-frown"></i>
                        </div>
                        <p> This category events Null ! </p>

                    </div>
                </div>
                {{/if}}

            </div>
            {{/if}}

        </div>

    </div>
</section>

<style>
    .EventIdSpan {
        font-size: 15px;
        font-weight: normal;
        color: rgb(82, 81, 81);
        font-style: italic;
    }
</style>
{{!-- active button --}}
<script>
    var current = document.getElementById('G-Events').className = "nav-links-a nav-links-active"

</script>

<script>
    $(document).ready(function () {
        $('#AllStudents').DataTable();
    });
</script>

<style>
    body {
        counter-reset: Serial;
        /* Set the Serial counter to 0 */
    }

    table {
        border-collapse: separate;
    }

    .auto-index td:first-child:before {
        counter-increment: Serial;
        /* Increment the Serial counter */
        content: counter(Serial);
        /* Display the counter */
    }
</style>