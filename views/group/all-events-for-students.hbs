<section>
    <div class="group-container">
        <div class="allGroupStudents-mainDiv">
            <div class="GroupAllCategory-header">
                <h4 class="adminHome-sectin-one-SubHeading">{{SessionName}} / {{Category}}</h4>
                <hr class="GroupallCategoryheader-hr">
            </div>
        </div>






        <div class="GroupAllStudents-content">


            <div class="GroupEvents-All-content " style="box-sizing: border-box;">
                {{#if AllEvents.[0]}}
                {{#each AllEvents}}
                <a id="mainDiv"
                    href="/group/events/{{../SessionName}}/{{../Category}}/all-events/{{this.EventId}}-{{this.EventName}}/all-students">
                    <div style="background-color: rgb(219, 219, 219);" class="allGroupCatetory-items">
                        <h4>{{this.EventName}} <span class="EventIdSpan">({{this.EventId}})</span> </h4>
                        <p>{{this.LimitStatus}}/{{this.EventLimit}}</p>
                    </div>
                </a>
                {{/each}}
                
                <div style="margin-top: 10px;">
                    <div class="pointTable-printbutton">
                        <button class="print-button" onclick="PrintDiv('print-container','nsaonline.in')"><i
                                class="bi bi-printer-fill"></i>
                            Print</button>

                    </div>
                    <h5 style="text-align: center; margin-top:5px"><span style="color: red; font-size:17px">*</span>
                        Save
                        your PDF in print
                        option</h5>
                </div>

                {{else}}
                <div class="GroupStudentsZero">
                    <div class="pointTabel-div">
                        <div class="pointTable-IconDiv">
                            <i class="bi bi-emoji-frown"></i>
                        </div>
                        <p> This category events Null ! </p>

                    </div>
                </div>

                {{/if}}

            </div>


        </div>

    </div>
</section>



{{!-- print side --}}
<div hidden id="print-container">

    <head>
        <link rel='stylesheet' href='/stylesheets/admin/print.css' />
        <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.5.0/font/bootstrap-icons.css">

    </head>
    <table class="printTable-border">
        <thead>
            <tr>
                <td>
                    <div class="printHeader">
                    </div>
                </td>
            </tr>
        </thead>
        <tbody>
            <tr>
                <td>
                    <div class="printContent">

                        <table class="auto-index print-Table">
                            {{#if AllEvents.[0]}}

                            <thead>
                                <tr>
                                    <th class="StudentsTable-TR-Slno">Sl no</th>
                                    <th class="StudentsTable-TR-ChestNo">Event ID</th>
                                    <th class="StudentsTable-TR-CIC">Title</th>
                                    <th class="StudentsTable-TR-Name">Event limit</th>
                                    <th class="StudentsTable-TR-Name">Students limit</th>

                                </tr>
                            </thead>
                            <tbody>
                                {{#each AllEvents}}
                                <tr>
                                    <td class="StudentsTable-TD-Slno"></td>
                                    <td class="StudentsTable-TD-ChestNo">{{this.EventId}}</td>
                                    <td class="StudentsTable-TD-CIC">{{this.EventName}}</td>
                                    <td class="StudentsTable-TD-Name">{{this.EventLimit}}</td>
                                    <td class="StudentsTable-TD-Name">{{this.LimitStatus}}</td>

                                </tr>
                                {{/each}}
                            </tbody>
                            {{else}}
                            <div class="pointTable-nodataDiv">
                                <p style="text-align: center;">No data available...</p>
                            </div>
                            {{/if}}
                        </table>

                    </div>
                </td>
            </tr>
        </tbody>
        <tfoot>
            <tr>
                <td>
                    <div class="printFooter">

                    </div>

                </td>
            </tr>
        </tfoot>
    </table>

    <div class="print-header-space">
        <div class="print-header-conternt">
            <div>
                <img src="/images/fest-logo/{{FestDetails.FestId}}.jpg" onerror="this.onerror=null;callfun(this);"
                    alt="Fest Logo">
                <div>
                    <h2>{{FestDetails.FestName}} </h2>
                    <h4>FestID : {{FestDetails.FestId}}</h4>
                    <p> Fest Date :{{FestDetails.FestDate}} </p>
                </div>
            </div>
            <div class="sessionTwo">
                <div>
                    <p style="font-weight: bold;"> NOORUL HUDA STUDENTS ASSOCIATION (NSA) </p>
                    <p> Run by: Majma'a Wafy College - Kavanur </p>
                    <p style="font-size: 14px;"> Rahmath nagar, Kavanur P.O, Areekode</p>
                </div>

            </div>
        </div>
        <div class="print-page-headline">
            <h4> - {{SessionName}} / {{Category}} / Events - </h4>
        </div>
    </div>

    <div class="print-footer-space">
        <i class="bi bi-globe"></i>
        <p>www.nsaonline.in</p>
        <i class="bi bi-envelope"></i>
        <p>nsamajmaakvr@gmail.com</p>
        <i class="bi bi-instagram"></i>
        <i class="bi bi-facebook"></i>
        <i class="bi bi-youtube"></i>
        <i class="bi bi-telegram"></i>
        <p>/follow us</p>
    </div>
</div>


<style>
    .EventIdSpan {
        font-size: 15px;
        font-weight: normal;
        color: rgb(82, 81, 81);
        font-style: italic;
    }
</style>
