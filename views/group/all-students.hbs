<section>
    <div class="group-container">
        <div class="allGroupStudents-mainDiv">
            <div class="GroupAllCategory-header">
                <h4 class="adminHome-sectin-one-SubHeading">{{SessionName}} students</h4>
                <hr class="GroupallCategoryheader-hr">
            </div>
        </div>
        <div class="GroupAllStudents-content">

            {{#if StudentActiveUndifined}}
            <div class="GroupStudentsZero">
                <div class="pointTabel-div">
                    <div class="pointTable-IconDiv">
                        <i class="bi bi-hourglass-top"></i>
                    </div>
                    <p>Pleace Wait... <br> The time to create students has not begun</p>
                </div>
            </div>
            {{else}} {{#if SessionActivation}}
            <div class="GroupStudentsZero">
                <div class="pointTabel-div">
                    <div class="pointTable-IconDiv">
                       <i class="bi bi-cloud-haze2-fill"></i>
                    </div>
                    <p>Pleace Wait... <br> This session not activated</p>
                </div>
            </div>
            {{else}}{{#if StudentsCoutZero}}
            <div class="GroupStudentsZero">
                <div class="pointTabel-div">

                    <p>You have not added any students...</p>
                    {{#if StudentActiveNull}}
                    <button style="background-color:var(--color-delete);"><i class="bi bi-hourglass-bottom"></i> Time
                        end </button>
                    {{else}}
                    <a href="/group/students/{{SessionName}}/add-students">
                        <button><i class="bi bi-plus-circle-dotted"></i> Create students</button>
                    </a>
                    {{/if}}
                </div>
            </div>
            {{else}}
            <div class="GroupStudents-Table-content">
                <div class="allStudents-SessionOne">
                    <p>Create new Student :</p>
                    {{#if StudentActiveNull}}
                    <a href=""><button style="background-color:var(--color-delete);"><i
                                class="bi bi-hourglass-bottom"></i> Time end</button></a>
                    {{else}}
                    <a href="/group/students/{{SessionName}}/add-students"><button><i
                                class="bi bi-plus-circle-dotted"></i> Add</button></a>
                    {{/if}}
                </div>
                <div class="allStudents-SessionTwo">
                    <table id="AllStudents" class="auto-index ">
                        <thead>
                            <tr>
                                <th class="StudentsTable-TR-Slno">Sl no</th>
                                <th class="StudentsTable-TR-ChestNo">Chest No</th>
                                <th class="StudentsTable-TR-CIC">CIC No</th>
                                <th class="StudentsTable-TR-Name">Full name</th>
                                <th class="StudentsTable-TR-Control">Control</th>
                            </tr>
                        </thead>
                        <tbody>
                            {{#each AllStudents}}
                            <tr>
                                <td class="StudentsTable-TD-Slno"></td>
                                <td class="StudentsTable-TD-ChestNo">{{this.ChestNo}}</td>
                                <td class="StudentsTable-TD-CIC">{{this.CicNo}}</td>
                                <td class="StudentsTable-TD-Name">{{this.FullName}}</td>
                                <td class="StudentsTable-TD-Control">
                                    <a title="View student"
                                        href="/group/students/{{../SessionName}}/{{this.ChestNo}}/view"><i
                                            class="bi bi-eye-fill"></i></a>
                                    {{#if ../editStatus}}
                                    <a title="Edit sudent" style="background-color: var(--color-edit);  "
                                        href="/group/students/{{../SessionName}}/{{this.ChestNo}}/edit"><i
                                            class="bi bi-pencil-square"></i></a>
                                    {{/if}}
                                    <a title="Delete student"
                                        onclick="return confirm('Are you want to delete - {{this.FullName}} ?')"
                                        href="/group/students/{{../SessionName}}/{{this.ChestNo}}/delete"
                                        style="background-color:var(--color-delete)"><i class="bi bi-trash"></i></a>
                                </td>
                            </tr>
                            {{/each}}

                        </tbody>
                    </table>
                </div>
            </div>
            <div style="margin-top: 10px;">
                <div class="pointTable-printbutton">
                    <button class="print-button" onclick="PrintDiv('print-container','nsaonline.in')"><i
                            class="bi bi-printer-fill"></i>
                        Print</button>
                    <button style="background-color: black;" class="print-button"
                        onclick="PrintDiv('print-containe','nsaonline.in')"><i class="bi bi-printer-fill"></i>
                        Print with Events</button>
                </div>
                <h5 style="text-align: center; margin-top:5px"><span style="color: red; font-size:17px">*</span> Save
                    your PDF in print
                    option</h5>
            </div>

            {{/if}}{{/if}}{{/if}}

            {{#if cicNOError}}
            <div class="groupAlertBorder">
                <div class="groupAlert-box-close-button">
                    <i class="bi bi-x-lg" onclick="closeAlet()"></i>
                </div>
                <div class="groupAlertHead">
                    <i class="bi bi-exclamation-circle"></i>
                    <h4>edit failed !</h4>
                </div>
                <div class="groupAletCountent">
                    <h5>{{cicNOError}}</h5>
                </div>
                <audio autoplay hidden src="/audio/alert/error alert.mp3"></audio>
            </div>
            {{/if}}
            {{#if studentEventTrueError}}
            <div class="groupAlertBorder">
                <div class="groupAlert-box-close-button">
                    <i class="bi bi-x-lg" onclick="closeAlet()"></i>
                </div>
                <div class="groupAlertHead">
                    <i class="bi bi-exclamation-circle"></i>
                    <h4>Error !</h4>
                </div>
                <div class="groupAletCountent">
                    <h5>{{studentEventTrueError}}</h5>
                </div>
                <audio autoplay hidden src="/audio/alert/error alert.mp3"></audio>
            </div>
            {{/if}}
        </div>

    </div>
</section>


