<section>
    <div class="group-container">
        <div class="allGroupStudents-mainDiv">
            <div class="GroupAllCategory-header">
                <h4 class="adminHome-sectin-one-SubHeading">{{EventId}} - {{EventName}} / View student</h4>
                <hr class="GroupallCategoryheader-hr">
            </div>
            <div class="groups-content">


                <div class="gorup-content-events">
                    {{#if EventStudents}}
                    <div class="allStudents-SessionOne">
                        <p>Create new student this event :</p>
                        {{#if StudentActiveNull}}
                        <a href=""><button style="background-color:var(--color-delete);"><i
                                    class="bi bi-hourglass-bottom"></i> Time end</button></a>
                        {{else}}
                        <a href="/group/students/{{SessionName}}/add-students"><button><i
                                    class="bi bi-plus-circle-dotted"></i> Add</button></a>
                        {{/if}}
                    </div>
                    <div class="allStudents-SessionTwo">
                        <table id="AllStudents" class="auto-index">
                            <thead>
                                <tr>
                                    <th class="StudentsTable-TR-Slno">Sl no</th>
                                    <th class="StudentsTable-TR-ChestNo">Chest no</th>
                                    <th class="StudentsTable-TR-ChestNo">CIC no</th>
                                    <th class="StudentsTable-TR-CIC">Full name </th>
                                    <th class="StudentsTable-TR-Control">Delete</th>
                                </tr>
                            </thead>
                            <tbody>

                                {{#each EventStudents}}
                                <tr>
                                    <td class="StudentsTable-TD-Slno"></td>
                                    <td class="StudentsTable-TD-ChestNo">{{this.ChestNo}}</td>
                                    <td class="StudentsTable-TD-CIC">{{this.CicNo}}</td>
                                    <td class="StudentsTable-TD-Name">{{this.FullName}}</td>
                                    <td><a href="/group/events/{{../SessionName}}/{{../CategoryName}}/all-events/{{../EventId}}-{{../EventName}}/all-students/{{this.ChestNo}}/delete-event"
                                            onclick="return confirm('Are you want to delete - {{this.FullName}} - this event ?')"><button
                                                class="pointCateoryDeletButton"> <i
                                                    class="bi bi-trash"></i></button></a>
                                    </td>
                                </tr>
                                {{/each}}



                            </tbody>
                        </table>
                        <div class="info">
                            <p style="text-align: center; font-style:italic; color:rgb(80, 80, 80);"><i
                                    style="color:rgb(194, 7, 7);" class="bi bi-info-circle-fill"></i> Once the result is
                                published, the student cannot be deleted</p>
                        </div>
                    </div>
                    {{#if Error}}
                    <div id="close" class="Alert-box-border">
                        <div>
                            <div class="Alert-box-close-button">
                                <i class="bi bi-x-lg" onclick="closeAlet()"></i>
                            </div>
                            <div class="Alert-box-header">
                                <i class="bi bi-exclamation-circle"></i>
                                <h4>Alert !</h4>
                            </div>
                            <div class="alert-box-content">
                                <h5>{{Error}}</h5>
                            </div>
                            <audio autoplay hidden src="/audio/alert/error alert.mp3"></audio>

                        </div>
                    </div>
                    {{/if}}
                    <div style="margin-top: 10px;">
                        <div class="pointTable-printbutton">
                            <button class="print-button" onclick="PrintDiv('print-container','nsaonline.in')"><i
                                    class="bi bi-printer-fill"></i>
                                Print</button>
                        </div>
                    </div>
                    {{else}}
                    <div class="GroupStudentsZero">
                        <div class="pointTabel-div">

                            <p>You have not added any students this event ...</p>
                            {{#if StudentActiveNull}}
                            <button style="background-color:var(--color-delete);"><i class="bi bi-hourglass-bottom"></i>
                                Time
                                end </button>
                            {{else}}
                            <a href="/group/events/{{SessionName}}/{{CategoryName}}/{{EventId}}-{{EventName}}">
                                <button style="margin-left: 70px;"><i class="bi bi-plus-circle-dotted"></i> Add
                                    students</button>
                            </a>
                            {{/if}}
                        </div>
                    </div>
                    {{/if}}
                </div>
            </div>
        </div>
    </div>
</section>


