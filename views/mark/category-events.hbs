<section>
    <div class="fest-container">
        <div class="evets-MainDiv">
            <div class="pointTable-header">
                <h4 class="adminHome-sectin-one-SubHeading"><a href="/fest-admin/{{FestDetails.FestId}}/mark"><span class="back-button"><i class="bi bi-chevron-left"></i> Back</span></a>{{FestDetails.Session}} / {{FestDetails.Category}} / All events</h4>
                <hr>
            </div>
            <div class="GroupAllStudents-content">

               

              
                <div class="GroupEvents-All-content " style="box-sizing: border-box;">
                    {{#if allEvents.[0]}}
                    {{#each allEvents}}
                    <a id="mainDiv"
                        href="/fest-admin/{{../FestDetails.FestId}}/mark/{{../FestDetails.Session}}-{{../FestDetails.Category}}/{{this.TypeOfEvent}}/{{this.EventId}}-{{this.EventName}}/add-mark">
                        <div class="allGroupCatetory-items">
                            <h4>{{this.EventName}} <span style="font-style: italic; color:gray">({{this.EventId}})</span>
                            {{#if this.Result}}
                            <span><i style="font-size: 16px; color:rgb(0, 97, 21);"
                            title="Result published" class="bi bi-check-circle-fill"></i></span>
                            {{/if}}
                            </h4>
                            <p style="background: none; color:black"><i class="bi bi-chevron-right"></i></p>
                        </div>
                    </a>
                    {{/each}}

                    {{else}}
                    <div class="GroupStudentsZero">
                        <div class="pointTabel-div">
                            <div class="pointTable-IconDiv">
                                <i class="bi bi-emoji-frown"></i>
                            </div>
                            <p> No event added this category ! </p>

                        </div>
                    </div>
                    {{/if}}

                </div>
               

            </div>
        </div>
    </div>
</section>

<script>
    var current = document.getElementById('add-mark').className = "sidebarmenus sidebarmenus-active"

</script>