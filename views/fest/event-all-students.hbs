        </thead>
        <tbody>
            <tr>
                <td>
                    <div class="printContent">
                        {{#if EventStudents.[0]}}


                        <table class="auto-index print-Table">
                            <thead>
                                <tr>
                                    <th>Sl no</th>
                                    <th>GroupId</th>
                                    <th>ChestNo</th>
                                    <th>CIC No</th>
                                    <th>Name</th>


                                </tr>
                            </thead>
                            <tbody>
                                {{#each EventStudents}}
                                <tr>
                                    <td></td>
                                    <!--leave it blank-->
                                    <td>{{this.GroupId}}</td>
                                    <td>{{this.ChestNo}}</td>
                                    <td>{{this.CicNo}}</td>
                                    <td>{{this.FullName}}</td>

                                </tr>
                                {{/each}}

                            </tbody>
                        </table>
                        {{else}}
                        <div class="pointTable-nodataDiv">
                            <p style="text-align: center;">No data available...</p>
