<section>
    <div class="fest-container">
        <div class="groupsmainDiv">
            <div class="pointTable-header">
                <h4 class="adminHome-sectin-one-SubHeading">{{GroupId}} / {{SessionName}} / {{studentEvents.ChestNo}} - {{studentEvents.FullName}}</h4>
                <hr>
            </div>
            <div class="groups-content">
                <div class="gorup-content-header">
                    <div class="session1">
                        <h3> {{studentEvents.CicNo}} - {{studentEvents.FullName}} ({{studentEvents.ChestNo}})</h3>

                        {{#if EventLimit.GeneralStageEventCount}}
                        <p> <span style="font-weight: 600;">Non general</span> | Stage event :
                            {{studentLimitCount.StageCount}}/{{EventLimit.StageEventCount}} | Offstage
                            event : {{studentLimitCount.OffStageCount}}/{{EventLimit.OffStageEventCount}}</p>
                        <p> <span style="font-weight: 600;">General</span> | Stage event :
                            {{studentLimitCount.GeneralStageCount}}/{{EventLimit.GeneralStageEventCount}} | Offstage
                            event : {{studentLimitCount.GeneralOffStageCount}}/{{EventLimit.GeneralOffStageEventCount}}
                        </p>
                        {{else}}
                        <p>Stage event : <span id="stagePlace"></span>/{{EventLimit.StageEventCount}} | Offstage
                            event : <span id="offstagePlace"></span>/{{EventLimit.OffStageEventCount}} </p>
                        {{/if}}
                    </div>
                </div>
                 <p hidden id="stage">{{studentLimitCount.StageCount}}</p>
                <p hidden id="offstage">{{studentLimitCount.OffStageCount}}</p>
                <p hidden id="generalstage">{{studentLimitCount.GeneralStageCount}}</p>
                <p hidden id="generaloffstage">{{studentLimitCount.GeneralOffStageCount}}</p>
                <div class="gorup-content-events">
                    <div class="allStudents-SessionTwo">
                        <table id="AllStudents" class="auto-index">
                            <thead>
                                <tr>
                                    <th class="StudentsTable-TR-Slno">Sl no</th>
                                    <th class="StudentsTable-TR-ChestNo">Event ID</th>
                                    <th class="StudentsTable-TR-CIC">Event </th>
                                    <th class="StudentsTable-TR-Name">Category</th>
                                    <th class="StudentsTable-TR-Name">Section</th>
                                    <th class="StudentsTable-TR-Control">pointTable</th>
                                    <th class="StudentsTable-TR-Control">Delete</th>
                                </tr>
                            </thead>
                            <tbody>

                                {{#each studentEvents.StageEvents}}
                                <tr>
                                    <td class="StudentsTable-TD-Slno"></td>
                                    <td class="StudentsTable-TD-ChestNo">{{this.EventId}}</td>
                                    <td class="StudentsTable-TD-CIC">{{this.EventName}}</td>
                                    <td class="StudentsTable-TD-Name">STAGE</td>
                                    <td class="StudentsTable-TD-Name">{{this.status}}</td>
                                    <td class="StudentsTable-TD-Name">{{this.PointCategoryName}}</td>
                                    <td><a href="/fest-admin/{{../FestDetails.FestId}}/groups/{{../GroupId}}/{{../SessionName}}/students/{{../studentEvents.ChestNo}}-{{this.EventId}}/delete-event"
                                            onclick="return confirm('Are you want to delete - {{this.EventName}} -  category ?')"><button
                                                class="pointCateoryDeletButton"> <i
                                                    class="bi bi-trash"></i></button></a>
                                    </td>
                                </tr>
                                {{/each}}
                                {{#each studentEvents.OffStageEvents}}
                                <tr>
                                    <td class="StudentsTable-TD-Slno"></td>
                                    <td class="StudentsTable-TD-ChestNo">{{this.EventId}}</td>
                                    <td class="StudentsTable-TD-CIC">{{this.EventName}}</td>
                                    <td class="StudentsTable-TD-Name">OFF STAGE</td>
                                    <td class="StudentsTable-TD-Name">{{this.status}}</td>
                                    <td class="StudentsTable-TD-Name">{{this.PointCategoryName}}</td>
                                    <td><a href="/fest-admin/{{../FestDetails.FestId}}/groups/{{../GroupId}}/{{../SessionName}}/students/{{../studentEvents.ChestNo}}-{{this.EventId}}/delete-event"
                                            onclick="return confirm('Are you want to delete - {{this.EventName}} -  category ?')"><button
                                                class="pointCateoryDeletButton"> <i
                                                    class="bi bi-trash"></i></button></a>
                                    </td>
                                </tr>
                                {{/each}}

                            </tbody>
                        </table>
                    </div>
                    <div class="pointTable-printbutton">
                        <button class="print-button" onclick="PrintDiv('print-container','nsaonline.in')"><i
                                class="bi bi-printer-fill"></i>
                            Print</button>


                    </div>
                </div>
            </div>
        </div>
    </div>
</section>


