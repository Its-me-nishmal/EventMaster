<head>
    <div id="sidebar" class="sidebarborder">
        <div class="sidebarheader">
            <div class="sidebarheaderlogoDiv">
            <img class="sidebarheaderLogo" style="height: 110px;" src="/images/fest-logo/{{FestDetails.FestId}}.jpg"  onerror="this.onerror=null;callfun(this);" alt="fest-logo">
            </div>
            <h3 class="sidebarheader-festName">{{FestDetails.FestName}}</h3>
            <p class="sidebarheader-festID">FestID : {{FestDetails.FestId}}</p>
            <i id="closeIcon" onclick="closeButton()" class="bi bi-x-lg sidebarheaderCloseIcon"></i>
        </div>
        <hr class="sidebarheader-hr">
        <div class="sidebarcontent">
            <div id="sidebarmenus-div" class="sidebarcontent-border">
                <a id="home" class="sidebarmenus" href="/fest-admin/{{FestDetails.FestId}}/home"><i class="bi bi-house-fill"></i> Home</a>
                <a id="event" class="sidebarmenus" href="/fest-admin/{{FestDetails.FestId}}/events"><i class="bi bi-calendar2-event-fill"></i> Events</a>
                <a id="point-table" class="sidebarmenus" href="/fest-admin/{{FestDetails.FestId}}/point-table"><i class="bi bi-table"></i> Point table</a>
                <a id="groups" class="sidebarmenus" href="/fest-admin/{{FestDetails.FestId}}/groups"><i class="bi bi-slack"></i> Groups</a>
                <a id="add-mark" class="sidebarmenus" href="/fest-admin/{{FestDetails.FestId}}/mark"><i class="bi bi-plus-lg"></i> Add mark</a>
                <a id="result" class="sidebarmenus" href="/fest-admin/{{FestDetails.FestId}}/result"><i class="bi bi-bar-chart-line-fill"></i> Result</a>
                <a id="control-panel" class="sidebarmenus" href="/fest-admin/{{FestDetails.FestId}}/control-panel"><i class="bi bi-toggles"></i> Control panel</a>
                <a id="settings" class="sidebarmenus" href="/fest-admin/{{FestDetails.FestId}}/settings"><i class="bi bi-gear-wide-connected"></i> Settings & Other</a>
                {{!-- <a class="sidebarmenus" href=""><i class="bi bi-house-fill"></i> About</a>
                <a class="sidebarmenus" href=""><i class="bi bi-house-fill"></i> Account</a> --}}
           
            </div>
        </div>
    </div>
</head>

<script>
    function closeButton(){
        var sidebar = document.getElementById('sidebar')
        var festHeader = document.getElementById('festHeader')
        var closeButton = document.getElementById('closeIcon')
        
        
        sidebar.className = "sidebarboarder-onClick-close";
        closeButton.className = "bi bi-x-lg sidebarheaderCloseIcon-onClick-close"
        festHeader.className = "sidebarMenueButton-onClick-close"
    }
</script>


<script>
    function callfun(obj) {
        var noimg = "/images/images/fest logo demo.jpg";
        obj.src = noimg;
    }
</script>



